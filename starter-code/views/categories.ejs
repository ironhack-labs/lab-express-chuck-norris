<div class="row">
    <div class="col s12 m6 l2">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <a class="btn btn-floating right" href="/"><i class="material-icons">home</i></a>
          <span class="card-title">CATEGORIES</span>
          <br>
          <p>Pick a category to see jokes related</p>
          <br>
          <div class="card-action">
            <a class="random-joke-link right" href="/random">Go to random joke</a>
          </div>
        </div>
        <div class="collection center">
          <% value.forEach((result) => { %>
            <a href="categories?cat=<%= result %>" class="collection-item"><%= result %></a>
            <% }) %>
          </div>
      </div>
    </div>
  </div>
